        (filled lime_juice__bottle.n.01_1 lime_juice.n.01_1)
        (filled vodka__bottle.n.01_1 vodka.n.01_1)
        (ontop cola__bottle.n.01_1 countertop.n.01_1)
        (ontop lime_juice__bottle.n.01_1 countertop.n.01_1) 
