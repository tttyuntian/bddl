        (inroom electric_refrigerator.n.01_1 kitchen) 
        (ontop agent.n.01_1 floor.n.01_1)
    )
    
    (:goal 
        (and 
            (forall 
                (?water_glass.n.02 - water_glass.n.02)
                (and 
                    (filled ?water_glass.n.02 ?cola.n.02_1)
                    (contains ?water_glass.n.02 ?lime_juice.n.01_1)
                    (contains ?water_glass.n.02 ?vodka.n.01_1)
