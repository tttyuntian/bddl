        (inroom floor.n.01_1 garden) 
        (ontop agent.n.01_1 floor.n.01_1)
    )
    
    (:goal 
        (and 
            (not 
                (covered ?pebble.n.01_1 ?dust.n.01_1)
            )
            (covered ?pebble.n.01_1 ?polish.n.03_1)
        )
    )
)