(define (problem polish_rocks-0)
    (:domain omnigibson)

    (:objects
     	pebble.n.01_1 - pebble.n.01
    	coffee_table.n.01_1 - coffee_table.n.01
    	polish.n.03_1 - polish.n.03
    	polish__bottle.n.01_1 - polish__bottle.n.01
